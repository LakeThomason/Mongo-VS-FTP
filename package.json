{
  "name": "Mongo_VS_FTP",
  "version": "1.0.0",
  "description": "A quick performance evaluation of MongoDB's ability to deliver images vs an FTP server",
  "main": "index.js",
  "scripts": {
    "client-dev": "webpack-dev-server --port 5000 --hot --inline --watch --progress",
    "server-dev": "opn http://localhost:8000 && nodemon ./app/server/public/server.js localhost 7000",
    "build": "webpack -p",
    "start": "nodemon ./app/server/public/server.js localhost 7000",
    "prestart": "npm install && webpack -p",
    "deploy": "mern-scripts deploy"
  },
  "engines": {
    "node": "^8.5.0"
  },
  "author": "Ya bois",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.3.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.2",
    "gridfs-stream": "^1.1.1",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.17",
    "multer": "^1.4.1",
    "multer-gridfs-storage": "^3.2.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^7.1.0"
  },
  "devDependencies": {
    "mern-scripts": "^0.1.7",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "nodemon": "^1.12.1",
    "opn-cli": "^3.1.0",
    "serve": "^6.4.0",
    "style-loader": "^0.19.0",
    "svg-react-loader": "^0.4.5",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
